<archetype xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0 http://maven.apache.org/xsd/archetype-1.0.0.xsd">
	<id>executable-jar-archetype</id>
	<sources>
		<source encoding="UTF-8">src/main/java/App.java</source>
	</sources>
	<resources>
		<resource encoding="UTF-8">src/main/resources/logback.xml</resource>
		<resource encoding="UTF-8">.classpath</resource>
		<resource encoding="UTF-8">.project</resource>
		<resource encoding="UTF-8">.settings/org.eclipse.core.resources.prefs</resource>
		<resource encoding="UTF-8">.settings/org.eclipse.jdt.core.prefs</resource>
		<resource encoding="UTF-8">.settings/org.eclipse.m2e.core.prefs</resource>
	</resources>
	<testSources>
		<source encoding="UTF-8">src/test/java/AppTest.java</source>
	</testSources>
	<testResources>
		<resource encoding="UTF-8">src/test/resources/logback-test.xml</resource>
	</testResources>
</archetype>
